body
{
	margin : 0px;	
}

#menu
{
	padding-bottom : 61px; /* placement du trait bas de la barre d'onglet */
	border-bottom : 1px solid; /* trait bas de la barre d'onglet*/
}

#onglets
{
	font-size : 45px; /* taille nom onglet*/
	list-style-type : none; /* enlève les puces de la liste */ 
	margin-right : 5px; /*distance des onglets du bord droit de l'écran */ 
}

#onglets li
{
    float : right; /* placement à droite des onglets */
    height : 50px; /* hauteur des onglets */
    background-color: #e0e7e3ff;
    border : 1px solid #b7c8c4;
	border-bottom : black;
	border-top-left-radius : 25px;
	border-top-right-radius : 25px;
	padding : 5px;
}

#onglets li.active
{
    border-bottom: 1px solid #b7c8c4;
    background-color: #fff;
}

#onglets li:hover 
{
	background-color : #fff; /* couleur de fond quand on passe la souris sur un onglet */
}

#onglets a
{
    display : block;
	color : #262e29ff;
    text-decoration : none;
	padding : 4px;
}

#onglets li.titre 
{
	font-size : 55px;
	float : left;
	background-color : #b7c8c4;
	border-bottom : #b7c8c4;
	border-color : #b7c8c4;
}

#MAP
{
	background: url("images/map.svg");
	background-repeat : no-repeat;
	background-position : top center;
}

#BODY_FondUni
{
	background:  url("images/uni.svg");
}





#ClasInd{

	list-style-type : none; /* enlève les puces de la liste */ 
	
}


#ClasInd p
{
    display : block;
}




.dropdown {
  position: relative;
  display: inline-block;
}
.dropdown-content {
  display: none;
  position: absolute;
  background-color: #f9f9f9;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  padding: 12px 16px;
}
.dropdown:hover .dropdown-content {
  display: block;
}